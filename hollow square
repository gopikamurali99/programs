let n = 5

for(let i=1; i<=n;i++){
    
          if(i===1 || i===n  ){
                 process.stdout.write("*".repeat(n)) 
             }
              else{
                 process.stdout.write("*"+" ".repeat(n-2)+"*")
             }
   
    console.log(" ")
}

output

***** 
*   * 
*   * 
*   * 
***** 
