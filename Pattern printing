/* ****   javascript program to print square stars 
   ****
   ****
   ****    */
   
   let n=4;
   for(let i=1;i<=n;i++){
       
       for(let j=1;j<=n;j++){
           
           process.stdout.write("*");
           
       }
       process.stdout.write("*\n");
   }
   
console.log("Try programiz.pro");
