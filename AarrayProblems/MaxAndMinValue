function maxAndmin(arr){
    let maxVal=arr[0];
    let minVal=arr[0];
    
    for(let i=0;i<=arr.length;i++){
        if(maxVal < arr[i]){
            maxVal=arr[i];
            
        }
        if(minVal > arr[i]){
            minVal=arr[i];
            
        }
    }
 return `minvalue is ${minVal} and maxvalue is ${maxVal}`   
}

let array=[9,3,6,10,7]
console.log(maxAndmin(array))
